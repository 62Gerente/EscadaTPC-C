POPULATE

/usr/local/pgsql/bin/psql -d tpcc -f clean.tpcc -U alfranio
time make populate FILE=config.soft DATABASE=jdbc:postgresql://localhost/tpcc USR=alfranio WAREHOUSE=1

START

make real-jmx FILE=config.soft

-EBclass escada.tpc.tpcc.TPCCEmulation -LOGconfig configuration.files/logger.xml -KEY true -CLI 3 -STclass escada.tpc.tpcc.TPCCStateTransition -DBclass escada.tpc.tpcc.database.transaction.postgresql.dbPostgresql -TRACEFLAG TRACE -PREFIX 3343-C -DBpath jdbc:postgresql://192.168.82.132:5433/tpcc -DBdriver org.postgresql.Driver -DBusr tpcc -DBpasswd tpcc -POOL 20 -MI 20 -FRAG 1 -RESUBMIT false
